  <script>
    document.documentElement.className = document.documentElement.className.replace(/\bno-js\b/g, '') + ' js ';
  </script>

  <!-- For all browsers -->
  <script>
    window.theme = 'light'
    try {
      if (!localStorage.getItem('colour-scheme')) {
        if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
          localStorage.setItem('colour-scheme', 'dark');
        } else {
          localStorage.setItem('colour-scheme', 'light');
        }
      }
      window.theme = localStorage.getItem('colour-scheme');
    } catch (e) {
      console.error("dark/light mode not supported because localStorage is not available")
    }
  </script>

